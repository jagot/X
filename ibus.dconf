[general]
engines-order=['xkb:us::eng', 'xkb:gr::gre', 'anthy', 'hangul', 'xkb:se::swe']
preload-engines=['xkb:us::eng', 'anthy', 'hangul', 'xkb:gr::gre', 'xkb:se::swe']
